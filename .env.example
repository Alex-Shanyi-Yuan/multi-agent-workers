# Confluence Configuration
CONFLUENCE_URL=https://your-instance.atlassian.net
CONFLUENCE_USERNAME=your-email@domain.com
CONFLUENCE_API_TOKEN=your-api-token

# OPENAI API
OPENAI_API_KEY=''
OPENAI_MODEL=gpt-4o

# Document Processing
MAX_PDF_SIZE_MB=10
MAX_DOCX_SIZE_MB=10
DOCUMENT_CACHE_DIR=./cache

# Log Analysis
LOG_PATTERN=\[(ERROR|WARN|INFO)\].*
MAX_LOG_DEPTH=5
LOG_HISTORY_DAYS=7

# Agent Configuration
TEMPERATURE=0.7
MAX_TOKENS=2000
MODEL_NAME=gpt-4o
